<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="30dp"
        android:height="30dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#24737E" />
        </shape>
    </item>
    <item
        android:width="30dp"
        android:height="30dp">
        <shape
            android:innerRadius="1px"
            android:shape="oval">
            <size
                android:width="2dp"
                android:height="2dp" />

            <gradient
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="@color/teal_200"
                android:gradientRadius="150dp"
                android:startColor="@color/white"
                android:type="radial" />

            <stroke
                android:width="1dp"
                android:color="#1F2E1B" />
            <corners android:radius="32dp" />
        </shape>
    </item>
</layer-list>